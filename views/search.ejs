<!DOCTYPE html>

<html>
    <head>
        <%- include("./partials/head") %>
    </head>

    <body>
        <%- include("./partials/title_block") %>

        <h2 style="margin: 15px;">Результатов поиска: <%= lesta.data.length %></h2>

        <div class="search_results">
            <ul>
                <% for (let account of lesta.data) { %>
                    <li style="list-style-type: none; padding: 7px;"><a style="color: #CED6D5;" href="/stat?id=<%= account.account_id%>"><%= account.nickname%></a></li>
                <% } %>
            </ul>
        </div>
        

    </body>

    <%- include("./partials/bottom") %> 
</html>